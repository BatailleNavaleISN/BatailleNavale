void setup(){
  size(1400,730);
  stroke(21, 96, 189);
  background(119,181,254);
}

void draw(){
  int espacey=(height-200)/2;      
  int espacex=(width-850)/2;
  
  for (int i=0;i<=2;i++){
    line(10,10+i*espacey,width-840,10+i*espacey);
  }
  
  for (int i=0;i<=2;i++){
    line(10+i*espacex,10,10+i*espacex,height-190);
  }
}
  
