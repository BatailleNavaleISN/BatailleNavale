int x;
int y;
int [][] grille=new int [3][3];
void setup(){
  size(600,600);
  background(54,158,220);
  placement();
}

void placement(){
  x=int(random(0,3));
  y=int(random(0,3));
  for (int x=0; x<3; x=x+1){
    for(int y=0; y<3; y=y+1){
      grille[x][y]=1;
      fill(255,50,50);
      rect(x*200,y*200,200,200);
    }
  }
}
